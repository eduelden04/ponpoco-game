{
  "routes": [
    {
      "route": "/js/*",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8"
      }
    },
    {
      "route": "/",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/js/*", "/*.js", "/*.css", "/*.png", "/*.jpg", "/*.gif", "/*.ico"]
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}